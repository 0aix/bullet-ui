  "description": "Ember 2 UI for Bullet - Realtime Streaming Record Tracing",
  "private": true,
  "main": "express-server.js",
  "keywords": [
    "Big Data",
    "Streaming",
    "Storm DRPC",
    "ember",
    "ember 2"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "clean": "rm -rf tmp dist node_modules bower_components",
    "build": "ember build --environment=production",
    "test": "PATH=./node_modules/.bin:$PATH; phantomjs --version; ember test",
    "local-build": "ember build",
    "local-test": "ember test --server",
